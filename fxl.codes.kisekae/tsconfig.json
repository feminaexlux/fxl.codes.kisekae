{
  "compilerOptions": {
    "moduleResolution": "Node",
    "module": "AMD",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outFile": "wwwroot/js/main.js",
    "allowSyntheticDefaultImports": true,
    "types": [
      "jquery"
    ],
    "lib": [
      "ESNext",
      "DOM"
    ]
  },
  "include": [
    "./Scripts/*"
  ],
  "exclude": [
    "node_modules"
  ]
}